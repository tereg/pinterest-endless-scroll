<div class="pin">
  <div class="card">
    <div class="card-block">
      <img class="card-img-top img-fluid main-image" src="<%=  pin["images"]["736x"]["url"] %>" alt="">
      <p class="card-text"> <%= pin["title"] %></p>
      <p class="card-text"> <%= pin["description"] %></p>
      <p class="card-text"> <%= pin["repin_count"] %></p>
      <% if pin["rich_summary"].present? == true %>
        <p class="card-text"> <%= pin["rich_summary"]["site_name"] %></p>
      <% end %>
      <p class="card-text"> <%= pin["pinner"]["full_name"] %></p>
      <p class="card-text"> <%= pin["board"]["name"] %></p>
      <img src="<%= pin["pinner"]["image_small_url"] %>" alt="">
    </div>
  </div>
</div>

